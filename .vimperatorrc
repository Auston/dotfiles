"2.3.1 (created: 2010/04/07 18:56:53)

"tab numbers
set guioptions=rbN

" Mappings
noremap j 5j
noremap k 5k

"Change tab
noremap <S-j> <C-p>
noremap <S-K> <C-n>

"back and forward
map <S-h> :back<CR>
map <S-i> :forward<CR>

"windows copy, past, all slect,undo
"noremap <C-c> <C-v><C-c>
inoremap <C-c> <C-v><C-c>
cnoremap <C-c> <C-v><C-c>

"noremap <C-v> <C-v><C-v>
inoremap <C-v> <C-v><C-v>
"coremap <C-v> <C-v><C-v>

"noremap <C-a> <C-v><C-a>
inoremap <C-a> <C-v><C-a>
cnoremap <C-a> <C-v><C-a>


noremap <C-u> <C-v><C-u>
noremap <C-s> <C-c>
noremap <C-a> <C-v><C-a>
cnoremap <C-x> <C-v><C-x>
inoremap <C-x> <C-v><C-x>



" PassThrough gmail and greader
autocmd LocationChange .* js modes.passAllKeys = /.*(mail\.google\.com|www\.google\.com\/reader).*/.test(buffer.URL)

"set runtimepath=/home/jary_p/.vimperator
"source! /home/jary_p/.vimperatorrc.local
