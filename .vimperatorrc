map A <Nop>

noremap > g$
noremap < g^
noremap , <C-6>

"tab numbers
set guioptions=rbN

" Mappings
noremap j 5j
noremap k 5k

"Change tab
noremap <S-j> <C-p>
noremap <S-K> <C-n>

"back and forward
map <S-h> :back<CR>
map <S-l> :forward<CR>

"windows copy, past, all slect,undo
noremap  <C-c> <C-v><C-c>
inoremap <C-c> <C-v><C-c>

"noremap  <C-v> <C-v><C-v>
inoremap <C-v> <C-v><C-v>

noremap  <C-a> <C-v><C-a>
inoremap <C-a> <C-v><C-a>
cnoremap <C-a> <C-v><C-a>


noremap  <C-b> <C-v><C-b>
inoremap <C-b> <C-v><C-b>
cnoremap <C-b> <C-v><C-b>

noremap  <C-d> <C-v><C-d>
inoremap <C-d> <C-v><C-d>
cnoremap <C-d> <C-v><C-d>

noremap  <C-o> <C-v><C-o>


set "editor=vim -f"
set nextpattern=\s*下一页|下一张|下一篇|下页|后页\s*,^\\bnext\\b,\\bnext\\b,^>$,^(>>|»)$,^(>|»),(>|»)$,\\bmore\\b
set previouspattern=\s*上一页|上一张|上一篇|上页|前页\s*,^\\bprev|previous\\b,\\bprev|previous\\b,^<$,^(<<|«)$,^(<|«),(<|«)$
set defsearch=google


" PassThrough gmail and greader
autocmd LocationChange .* js modes.passAllKeys = /.*(mail\.google\.com|www\.google\.com\/reader).*/.test(buffer.URL)

